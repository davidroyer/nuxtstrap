{
  "name": "nuxtstrap",
  "version": "0.0.21",
  "description": "NuxtStrap App for Nuxt Quickstart Guide",
  "author": "David Royer",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:api": "yarn api && nuxt",
    "build": "NODE_ENV=production nuxt build",
    "start": "NODE_ENV=production nuxt start",
    "generate": "nuxt generate",
    "deploy:now": "now --dotenv --public && now alias"
  },
  "dependencies": {
    "@nuxtjs/axios": "latest",
    "@nuxtjs/device": "^1.2.2",
    "@nuxtjs/dotenv": "latest",
    "@nuxtjs/toast": "latest",
    "body-parser": "^1.18.3",
    "bootstrap": "latest",
    "bootstrap-vue": "latest",
    "express-session": "^1.15.6",
    "netlify-identity-widget": "latest",
    "nuxt": "lastest"
  },
  "devDependencies": {
    "cross-env": "^5.0.1",
    "json-server": "^0.13.0",
    "node-sass": "^4.8.3",
    "sass-loader": "^7.0.1"
  },
  "now": {
    "name": "nuxtqs-nuxtstrap",
    "alias": [
      "nuxtqs-nuxtstrap",
      "nuxtstrap.nuxtjs-quickstart.com"
    ]
  }
}
